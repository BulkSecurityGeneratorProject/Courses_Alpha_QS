<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Turma.
    -->
    <changeSet id="20161030195747-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="disciplina_id"
                                 baseTableName="turma"
                                 constraintName="fk_turma_disciplina_id"
                                 referencedColumnNames="id"
                                 referencedTableName="disciplina"/>

        <addForeignKeyConstraint baseColumnNames="professor_id"
                                 baseTableName="turma"
                                 constraintName="fk_turma_professor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="professor"/>

        <addForeignKeyConstraint baseColumnNames="turmas_id"
                                 baseTableName="turma_aluno"
                                 constraintName="fk_turma_aluno_turmas_id"
                                 referencedColumnNames="id"
                                 referencedTableName="turma"/>
        <addForeignKeyConstraint baseColumnNames="alunos_id"
                                 baseTableName="turma_aluno"
                                 constraintName="fk_turma_aluno_alunos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aluno"/>
        
        <addForeignKeyConstraint baseColumnNames="turmas_id"
                                 baseTableName="turma_sala"
                                 constraintName="fk_turma_sala_turmas_id"
                                 referencedColumnNames="id"
                                 referencedTableName="turma"/>
        <addForeignKeyConstraint baseColumnNames="salas_id"
                                 baseTableName="turma_sala"
                                 constraintName="fk_turma_sala_salas_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sala"/>
        
    </changeSet>
</databaseChangeLog>
