<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Avaliacao.
    -->
    <changeSet id="20161030210216-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="avaliacaos_id"
                                 baseTableName="avaliacao_aluno"
                                 constraintName="fk_avaliacao_aluno_avaliacaos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="avaliacao"/>
        <addForeignKeyConstraint baseColumnNames="alunos_id"
                                 baseTableName="avaliacao_aluno"
                                 constraintName="fk_avaliacao_aluno_alunos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aluno"/>
        
        <addForeignKeyConstraint baseColumnNames="disciplina_id"
                                 baseTableName="avaliacao"
                                 constraintName="fk_avaliacao_disciplina_id"
                                 referencedColumnNames="id"
                                 referencedTableName="disciplina"/>

    </changeSet>
</databaseChangeLog>
